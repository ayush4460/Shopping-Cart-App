{"ast":null,"code":"import{createReducer}from\"@reduxjs/toolkit\";export var cartReducer=createReducer({cartItems:[],subTotal:0,shipping:0,tax:0,total:0},{addToCart:function addToCart(state,action){var item=action.payload;var isItemExist=state.cartItems.find(function(i){return i.id===item.id;});if(isItemExist){state.cartItems.forEach(function(i){if(i.id===item.id)i.quantity+=1;});}else{state.cartItems.push(item);}},decrement:function decrement(state,action){var item=state.cartItems.find(function(i){return i.id===action.payload;});if(item.quantity>1){state.cartItems.forEach(function(i){if(i.id===item.id)i.quantity-=1;});}},deleteFromCart:function deleteFromCart(state,action){state.cartItems=state.cartItems.filter(function(i){return i.id!==action.payload;});},calculatePrice:function calculatePrice(state){var sum=0;state.cartItems.forEach(function(i){return sum+=i.price*i.quantity;});state.subTotal=sum;state.shipping=state.subTotal>=50000?0:2000;state.tax=+(state.subTotal*0.18).toFixed();state.total=state.subTotal+state.tax+state.shipping;}});","map":{"version":3,"names":["createReducer","cartReducer","cartItems","subTotal","shipping","tax","total","addToCart","state","action","item","payload","isItemExist","find","i","id","forEach","quantity","push","decrement","deleteFromCart","filter","calculatePrice","sum","price","toFixed"],"sources":["C:/Web Apps/Shopping-Cart-App/src/redux/reducer.js"],"sourcesContent":["import { createReducer } from \"@reduxjs/toolkit\";\r\n\r\nexport const cartReducer = createReducer(\r\n  {\r\n    cartItems: [],\r\n    subTotal: 0,\r\n    shipping: 0,\r\n    tax: 0,\r\n    total: 0,\r\n  },\r\n  {\r\n    addToCart: (state, action) => {\r\n      const item = action.payload;\r\n      const isItemExist = state.cartItems.find((i) => i.id === item.id);\r\n\r\n      if (isItemExist) {\r\n        state.cartItems.forEach((i) => {\r\n          if (i.id === item.id) i.quantity += 1;\r\n        });\r\n      } else {\r\n        state.cartItems.push(item);\r\n      }\r\n    },\r\n\r\n    decrement: (state, action) => {\r\n      const item = state.cartItems.find((i) => i.id === action.payload);\r\n      if (item.quantity > 1) {\r\n        state.cartItems.forEach((i) => {\r\n          if (i.id === item.id) i.quantity -= 1;\r\n        });\r\n      }\r\n    },\r\n\r\n    deleteFromCart: (state, action) => {\r\n      state.cartItems = state.cartItems.filter((i) => i.id !== action.payload);\r\n    },\r\n\r\n    calculatePrice: (state) => {\r\n      let sum = 0;\r\n      state.cartItems.forEach((i) => (sum += i.price * i.quantity));\r\n      state.subTotal = sum;\r\n      state.shipping = state.subTotal >= 50000 ? 0 : 2000;\r\n      state.tax = +(state.subTotal * 0.18).toFixed();\r\n      state.total = state.subTotal + state.tax + state.shipping;\r\n    },\r\n  }\r\n);\r\n"],"mappings":"AAAA,OAASA,aAAa,KAAQ,kBAAkB,CAEhD,MAAO,IAAM,CAAAC,WAAW,CAAGD,aAAa,CACtC,CACEE,SAAS,CAAE,EAAE,CACbC,QAAQ,CAAE,CAAC,CACXC,QAAQ,CAAE,CAAC,CACXC,GAAG,CAAE,CAAC,CACNC,KAAK,CAAE,CACT,CAAC,CACD,CACEC,SAAS,CAAE,SAAAA,UAACC,KAAK,CAAEC,MAAM,CAAK,CAC5B,GAAM,CAAAC,IAAI,CAAGD,MAAM,CAACE,OAAO,CAC3B,GAAM,CAAAC,WAAW,CAAGJ,KAAK,CAACN,SAAS,CAACW,IAAI,CAAC,SAACC,CAAC,QAAK,CAAAA,CAAC,CAACC,EAAE,GAAKL,IAAI,CAACK,EAAE,GAAC,CAEjE,GAAIH,WAAW,CAAE,CACfJ,KAAK,CAACN,SAAS,CAACc,OAAO,CAAC,SAACF,CAAC,CAAK,CAC7B,GAAIA,CAAC,CAACC,EAAE,GAAKL,IAAI,CAACK,EAAE,CAAED,CAAC,CAACG,QAAQ,EAAI,CAAC,CACvC,CAAC,CAAC,CACJ,CAAC,IAAM,CACLT,KAAK,CAACN,SAAS,CAACgB,IAAI,CAACR,IAAI,CAAC,CAC5B,CACF,CAAC,CAEDS,SAAS,CAAE,SAAAA,UAACX,KAAK,CAAEC,MAAM,CAAK,CAC5B,GAAM,CAAAC,IAAI,CAAGF,KAAK,CAACN,SAAS,CAACW,IAAI,CAAC,SAACC,CAAC,QAAK,CAAAA,CAAC,CAACC,EAAE,GAAKN,MAAM,CAACE,OAAO,GAAC,CACjE,GAAID,IAAI,CAACO,QAAQ,CAAG,CAAC,CAAE,CACrBT,KAAK,CAACN,SAAS,CAACc,OAAO,CAAC,SAACF,CAAC,CAAK,CAC7B,GAAIA,CAAC,CAACC,EAAE,GAAKL,IAAI,CAACK,EAAE,CAAED,CAAC,CAACG,QAAQ,EAAI,CAAC,CACvC,CAAC,CAAC,CACJ,CACF,CAAC,CAEDG,cAAc,CAAE,SAAAA,eAACZ,KAAK,CAAEC,MAAM,CAAK,CACjCD,KAAK,CAACN,SAAS,CAAGM,KAAK,CAACN,SAAS,CAACmB,MAAM,CAAC,SA